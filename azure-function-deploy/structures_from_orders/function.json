{
    "bindings": [
        {
            "authLevel": "function",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "get",
                "post"
            ],
            "route": "structures-from-orders"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        },
        {
            "name": "myTimer",
            "type": "timerTrigger",
            "direction": "in",
            "schedule": "30 */10 * * * *"
        }
    ]
}