{
  "name": "eve-data-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://sidarthus89.github.io/EVE-Data-Site",
  "scripts": {
    "publish": "npm run predeploy && npm run deploy",
    "predeploy": "vite build && node redirect.js",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "worker:dev": "wrangler dev",
    "worker:deploy": "wrangler deploy",
    "worker:kv": "wrangler kv:key list --namespace-id b1980b4449db416d839e3adff0359989",
    "db:reset": "npx wrangler d1 execute eve_market --remote --file sql/schema.sql"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "dotenv": "^16.0.0",
    "mssql": "^9.1.1",
    "node-fetch": "^2.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^16.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "mssql": "^9.1.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "vite": "^7.0.4",
    "wrangler": "^4.28.1"
  }
